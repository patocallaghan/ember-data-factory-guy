{
  "name": "ember-data-factory-guy",
  "version": "3.9.8",
  "description": "Factories for testing Ember applications using EmberData",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "author": "Daniel Sudol <dansudol@yahoo.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/adopted-ember-addons/ember-data-factory-guy"
  },
  "homepage": "https://github.com/adopted-ember-addons/ember-data-factory-guy",
  "scripts": {
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "start": "ember server",
    "build": "ember build --environment=production",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "license": "MIT",
  "dependencies": {
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "pretender": "^3.4.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.0",
    "active-model-adapter": "2.2.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^1.6.0",
    "ember-cli": "~3.20.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deprecation-workflow": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-data": "~3.20.4",
    "ember-data-model-fragments": "4.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-django-adapter": "2.1.0",
    "ember-fetch": "^6.5.1",
    "ember-inflector": "~3.0.0",
    "ember-load-initializers": "^2.1.1",
    "ember-resolver": "^8.0.0",
    "ember-sinon": "4.0.0",
    "ember-source": "~3.20.2",
    "ember-template-lint": "^2.9.1",
    "eslint": "^7.5.0",
    "loader.js": "4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0"
  },
  "resolutions": {
    "ember-data": "3.20.4"
  },
  "keywords": [
    "ember-addon",
    "testing",
    "factories",
    "ember-data"
  ],
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "main": "index.js"
}
